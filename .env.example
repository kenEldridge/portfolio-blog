# Data Access Control
# Set to 'true' to include restricted datasets (Yahoo Finance - Personal/Research Use)
# Set to 'false' for public deployments (GitHub Pages) to exclude restricted data
# Default: true (for local development)
INCLUDE_RESTRICTED_DATA=true
